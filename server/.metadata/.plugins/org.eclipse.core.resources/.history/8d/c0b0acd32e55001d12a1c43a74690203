package Conponent;

import java.util.Arrays;

public class ChatRoom {
	public static final int MAX_CHATTING_ROOM = 100;
	public static final int MAX_CHATTING_LOG = 10;
	static int roomId;
	static String[] chatInfo = new String[MAX_CHATTING_LOG];
	static String[][] chatLog = new String[MAX_CHATTING_ROOM][MAX_CHATTING_LOG];
	
	public static void main(String[] args) {
		chatInfo[0] = "id";
		chatInfo[1] = "description";
		chatInfo[2] = "time";
		
		chatLog[0] = chatInfo.clone();
		System.out.println(Arrays.deepToString(chatInfo));
	}
}
