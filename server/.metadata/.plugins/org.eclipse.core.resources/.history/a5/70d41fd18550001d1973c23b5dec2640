package socket;

import option.Option;

public class Saver {
	public Saver() {
		
	}
	
	public Saver(Option user,Option[] list) {
		for (int i = 0; i < list.length; i++) {
			if (list[i].getName() == user.getName()) {
				list[i].optionCopy(user);
			}
		}
	}
	
	public static void save(Option user,Option[] list) {
		for (int i = 0; i < list.length; i++) {
			if (list[i].getName() == user.getName()) {
				list[i].optionCopy(user);
				System.out.println("유저 저장 성공" + user.getName());
			}
		}
	}

}
