package 과제정리;

public class h3 {
	public static int CM(int N) {
		if (N == 1) {
			return 2;
		}
		if (N == 2) {
			return 3;
		}
		if (N > 2) {
			return CM(N-1) + CM(N-2);
		}
		return -1;
	}
	
	public static int F(int N) {
		if (N == 1) {
			return 2;
		}
		if (N == 2) {
			return 3;
		}
		if (N > 2) {
			int[] A = new int[N];
			A[0] = 2;
			A[1] = 3;
			for (int i = 0; i < A.length; i++) {
				A[i] = A[i-1] + A[i-2];
			}
			return A[N];
		}
		return -1;
	}
	
	public static void main(String[] args) {
		System.out.println(CM(5));
		System.out.println(F(5));
	}
}
