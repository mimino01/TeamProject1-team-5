package 과제정리;

public class h302 {
	private static int CN(int[] A,int i,int n) {
		if (A.length <= i) {
			return 0;
		} else if (A[i] == n) {
			return CN(A, i++, n) + 1;
		} else {
			return CN(A, i++, n);
		}
	}
	
	public static void main(String[] args) {
		int[] a = {1,3,4,2,5,6,1,2,1,6};
		System.out.println(CN(a, 0, 1));
	}
}
