package 과제정리;

public class h402 {
	public static int P(int[] A) {
		int result = 0;
		for (int i = 0; i < A.length; i++) {
			for (int j = i; j < A.length; j++) {
				if (A[i] < A[j]) {
					result++;
				}
			}
		}
		return result;
	}
	
	public static void main(String[] args) {
		int[] A = {82,35,27,96,73,14,58,49};
		System.out.println("억지" + P(A));
	}
}
